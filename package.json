{
  "name": "clean-console",
  "version": "0.0.0-semantic-release",
  "description": "Quickly loads remote url in phantomjs to check of JavaScript console errors",
  "main": "index.js",
  "bin": {
    "clean-console": "index.js"
  },
  "files": [
    "index.js",
    "bin",
    "History.md",
    "lib"
  ],
  "scripts": {
    "pretest": "gulp",
    "test": "npm run unit",
    "posttest": "npm run e2e",
    "basic": "node index.js -i examples/basic/index.html -o",
    "unit": "gt test/test.js",
    "e2e": "gt test/e2e.js --output",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "commit-wizard",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/clean-console.git"
  },
  "keywords": [
    "javascript",
    "phantomjs",
    "console",
    "test"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "gitHead": "983daf4827399a19ed85742927c0bf3775c47d5d",
  "readmeFilename": "README.md",
  "dependencies": {
    "check-more-types": "2.22.0",
    "console.json": "0.2.1",
    "optimist": "0.6.1",
    "phantomjs-prebuilt": "2.1.11",
    "q": "2.0.3",
    "quote": "0.4.0",
    "update-notifier": "0.1.10"
  },
  "devDependencies": {
    "gt": "0.10.0",
    "gulp": "3.9.1",
    "gulp-jshint": "1.10.0",
    "jshint-stylish": "2.2.0",
    "pre-git": "3.10.0",
    "semantic-release": "6.3.0"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
